{
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "disable-tls": true,
    "secure-http": false
  },
  "require": {
    "php": "8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "aura/router": "^3.1",
    "f16inkin/vulpix": "^1.0",
    "firebase/php-jwt": "^5.2",
    "laminas/laminas-diactoros": "^2.2",
    "laminas/laminas-httphandlerrunner": "^1.1",
    "laminas/laminas-stratigility": "^3.2",
    "php-di/php-di": "^6.0",
    "ramsey/uuid": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Sandbox\\Application\\": "src/Application/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "scripts": {
    "test": "phpunit --color=always"
  }
}
